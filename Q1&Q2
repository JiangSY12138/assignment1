#练习一
>a <- 1:1000 #赋值
> b <- 1/a #取倒数
> res1 <- atan(b) #求正切
> 
> x <- 1:1000 #赋值
> y <- atan(1/x) #求倒数正切
> z <- 1/tan(y) #求切线倒数

